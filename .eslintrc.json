{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn",
    "semi": "off",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "no-extra-boolean-cast": "off",
    "no-empty": "warn",
    "no-debugger": "warn"
  }
}